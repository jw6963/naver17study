package day1211;

public class Ex2DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int kor = 90;
		int eng = 93;
		System.out.println("total= "+kor+eng); // String + int + int : 묵시적 형변환에 의해서 String + int = String
		// int + int = int이므로 먼저 계산을 해야만 한다.
		System.out.println("total= "+ (kor+eng));
		
		// 진법 변환
		int a=056; // 앞에 0을 붙이면 8진수로 인식
		int b=0x12a; // 앞에 0x를 붙이면 16진수로 인식
		System.out.println("a= "+a); // 8진수를 10진수로 변경: 5*8+6*1 = 46
		System.out.println("b= "+b); // 16진수를 10진수로 변경: 1*16제곱+2*16+10*1 = 
	}	

}
