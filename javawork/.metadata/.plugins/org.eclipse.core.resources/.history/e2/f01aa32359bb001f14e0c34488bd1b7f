	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Loop:
			for(int i=1; i<=3; i++) {
				for (int j=1; j<=4; j++) {
//					if(i==2) {
//						continue Loop;
//					}
//					if(j==3) {
//						continue Loop; // i++로 이동, j가 1,2만 출력
//					}
					if(j==3) {
						break; // 위와 같다
					}
					System.out.println("i="+i+"j="+j);
				}
			}

	}

}
