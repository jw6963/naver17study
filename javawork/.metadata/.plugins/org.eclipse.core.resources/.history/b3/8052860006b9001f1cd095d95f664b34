package day1213;

public class Ex10For {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=1; i<=5; i++) { // 3번째 값은 증감연산자나 대입연산자만 올 수 있다
			System.out.print(i+" ");
		}
		System.out.println();
		for (int i=1; i<=10; i+=2) {
			System.out.print(i+" ");
		}
		for (int i=10; i>=1; i--) {
			System.out.print(i+" ");
		}
		System.out.println();
		System.out.println();
		for (int i=10; i<=100; i+=10) {
			System.out.print(i+" ");
		}
		System.out.println();
	}

}
