package day1211;

public class Ex1DataType {

	public static void main(String[] args) {
		byte a = 127;
		byte b = (byte)128;
		// 426 = 256 + 128 + 32 + 8 + 2 = 8 7 5 3 1
		byte c = (byte)426;
		byte d = (byte)161;
		System.out.println("a= "+a);
		System.out.println("b= "+b);
		System.out.println("d="+d);
	}
}