package day1217;

import java.util.ArrayList;
import java.util.List;

public class Book125 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] sum = {0};
		
		List<Integer> a = new ArrayList<>();
		a.forEach((number)->{
			
			sum[0] +=number;
		});
		System.out.println(sum);

	}

}
