package day1213;

public class Ex6While {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=65;
		while(a<=90) {
//			System.out.printf("%c",a++);
			System.out.print((char)a++);
		}
		System.out.println();
		char b='a';
		// do while을 이용해서 a부터 z까지 출력하시옿
		do {
			System.out.print(b);
			b++;
		} while (b<123);
	}

}

